    <types name="zfs" documentation="&lt;p>Manage zfs. Create destroy and set properties on zfs instances.&#xA;&lt;/p>&#xA;&lt;p>**Autorequires:** If Puppet is managing the zpool at the root of this zfs instance, the zfs resource will autorequire it. If Puppet is managing any parent zfs instances, the zfs resource will autorequire them.&#xA;&lt;/p>">
      <properties name="sharenfs" documentation="&lt;p>The sharenfs property. Values: on | off | share(1M) options&#xA;&lt;/p>"/>
      <properties name="volsize" documentation="&lt;p>The volsize property. Values: &lt;size>&#xA;&lt;/p>"/>
      <properties name="atime" documentation="&lt;p>The atime property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="aclmode" documentation="&lt;p>The aclmode property. Values: discard | groupmask | passthrough&#xA;&lt;/p>"/>
      <properties name="xattr" documentation="&lt;p>The xattr property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="checksum" documentation="&lt;p>The checksum property. Values: on | off | fletcher2 | fletcher4 | sha256&#xA;&lt;/p>"/>
      <properties name="setuid" documentation="&lt;p>The setuid property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="version" documentation="&lt;p>The version property. Values: 1 | 2 | 3 | 4 | current&#xA;&lt;/p>"/>
      <properties name="shareiscsi" documentation="&lt;p>The shareiscsi property. Values: on | off | type=&lt;type>&#xA;&lt;/p>"/>
      <properties name="recordsize" documentation="&lt;p>The recordsize property. Values: 512 to 128k, power of 2&#xA;&lt;/p>"/>
      <properties name="secondarycache" documentation="&lt;p>The secondarycache property. Values: all | none | metadata&#xA;&lt;/p>"/>
      <properties name="quota" documentation="&lt;p>The quota property. Values: &lt;size> | none&#xA;&lt;/p>"/>
      <properties name="zoned" documentation="&lt;p>The zoned property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="aclinherit" documentation="&lt;p>The aclinherit property. Values: discard | noallow | restricted | passthrough | passthrough-x&#xA;&lt;/p>"/>
      <properties name="canmount" documentation="&lt;p>The canmount property. Values: on | off | noauto&#xA;&lt;/p>"/>
      <properties name="sharesmb" documentation="&lt;p>The sharesmb property. Values: on | off | sharemgr(1M) options&#xA;&lt;/p>"/>
      <properties name="compression" documentation="&lt;p>The compression property. Values: on | off | lzjb | gzip | gzip-[1-9] | zle&#xA;&lt;/p>"/>
      <properties name="primarycache" documentation="&lt;p>The primarycache property. Values: all | none | metadata&#xA;&lt;/p>"/>
      <properties name="nbmand" documentation="&lt;p>The nbmand property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="snapdir" documentation="&lt;p>The snapdir property. Values: hidden | visible&#xA;&lt;/p>"/>
      <properties name="copies" documentation="&lt;p>The copies property. Values: 1 | 2 | 3&#xA;&lt;/p>"/>
      <properties name="mountpoint" documentation="&lt;p>The mountpoint property. Values: &lt;path> | legacy | none&#xA;&lt;/p>"/>
      <properties name="logbias" documentation="&lt;p>The logbias property. Values: latency | throughput&#xA;&lt;/p>"/>
      <properties name="reservation" documentation="&lt;p>The reservation property. Values: &lt;size> | none&#xA;&lt;/p>"/>
      <properties name="refquota" documentation="&lt;p>The refquota property. Values: &lt;size> | none&#xA;&lt;/p>"/>
      <properties name="refreservation" documentation="&lt;p>The refreservation property. Values: &lt;size> | none&#xA;&lt;/p>"/>
