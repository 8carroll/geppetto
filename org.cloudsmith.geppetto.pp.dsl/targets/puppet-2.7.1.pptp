      <properties name="devices" documentation="&lt;p>The devices property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="vscan" documentation="&lt;p>The vscan property. Values: on | off&#xA;&lt;/p>"/>
      <properties name="readonly" documentation="&lt;p>The readonly property. Values: on | off&#xA;&lt;/p>"/>
